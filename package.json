{
  "name": "nextjs-template",
  "version": "1.7.2",
  "engines": {
    "node": ">=16"
  },
  "description": "Opinionated Next.js template configured with TypeScript, Chakra UI, Redux Toolkit and other devtools",
  "repository": {
    "type": "git",
    "url": "https://github.com/Howard86/nextjs-template"
  },
  "author": "Howard Tai <howard@howardism.dev>",
  "license": "MIT",
  "keywords": [
    "template",
    "next.js",
    "typescript",
    "chakra-ui",
    "framer-motion",
    "redux",
    "redux-toolkit",
    "rtk-query",
    "jest",
    "eslint",
    "prettier",
    "lint-staged",
    "husky",
    "commitlint",
    "msw"
  ],
  "bugs": {
    "url": "https://github.com/Howard86/nextjs-template/issues"
  },
  "homepage": "https://github.com/Howard86/nextjs-template",
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "check-all": "yarn type-check && yarn lint && yarn format",
    "type-check": "tsc --noEmit",
    "lint": "TIMING=1 next lint",
    "format": "prettier --write --list-different .",
    "gen:theme-typings": "chakra-cli tokens src/common/theme/index.ts",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "prepare": "husky install"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.5.5",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@reduxjs/toolkit": "^1.9.3",
    "framer-motion": "^10.10.0",
    "next": "^13.2.4",
    "next-api-handler": "^0.4.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^2.3.0",
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@next/bundle-analyzer": "^13.2.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@types/react-redux": "^7.1.25",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/webpack-env": "^1.18.0",
    "eslint": "^8.37.0",
    "eslint-config-howardism": "^0.1.0",
    "graphql": "^16.6.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.1",
    "msw": "^1.2.1",
    "prettier": "^2.8.7",
    "typescript": "5.0.3",
    "whatwg-fetch": "^3.6.2"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
