{
  "name": "nextjs-template",
  "version": "1.7.1",
  "description": "Opinionated Next.js template configured with TypeScript, Chakra UI, Redux Toolkit and other devtools",
  "repository": {
    "type": "git",
    "url": "https://github.com/Howard86/nextjs-template"
  },
  "author": "Howard Tai <howard@howardism.dev>",
  "license": "MIT",
  "keywords": [
    "template",
    "next.js",
    "typescript",
    "chakra-ui",
    "framer-motion",
    "redux",
    "redux-toolkit",
    "rtk-query",
    "jest",
    "eslint",
    "prettier",
    "lint-staged",
    "husky",
    "commitlint",
    "msw"
  ],
  "bugs": {
    "url": "https://github.com/Howard86/nextjs-template/issues"
  },
  "homepage": "https://github.com/Howard86/nextjs-template",
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "check-all": "yarn type-check && yarn lint && yarn format",
    "type-check": "tsc --noEmit",
    "lint": "next lint",
    "format": "prettier --write .",
    "gen:theme-typings": "chakra-cli tokens src/app/theme/index.ts",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "prepare": "husky install"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.4.6",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@reduxjs/toolkit": "^1.9.1",
    "framer-motion": "^7.10.3",
    "next": "^13.0.7",
    "next-api-handler": "^0.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^2.3.0",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@next/bundle-analyzer": "^13.0.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-redux": "^7.1.24",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "eslint": "^8.30.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^13.0.7",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.7",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "graphql": "^16.6.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^13.1.0",
    "msw": "^0.49.2",
    "prettier": "^2.8.1",
    "typescript": "^4.9.4",
    "whatwg-fetch": "^3.6.2"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
